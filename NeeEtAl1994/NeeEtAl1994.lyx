\begin_layout Standard
To calculate the likelihood of a phylogeny and parameters, the phylogeny
 is cut into pieces (
\begin_inset CommandInset ref
LatexCommand ref
reference "fig:NeeAtAlFig2"

\end_inset

), as all we need are braching times (
\begin_inset Formula $ts$
\end_inset

) and branch lengths (
\begin_inset Formula $xs$
\end_inset

).
 The shape of the tree is unimportant for this model.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
u\left(\lambda,\mu,x\right)=\frac{\lambda\left(1-exp\left(-\left(\lambda-\mu\right)x\right)\right)}{\lambda-\mu\left(-\left(\lambda-\mu\right)x\right)}\label{eq:NeeEtAlEq1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
P\left(\lambda,\mu,t,T\right)=\frac{\lambda-\mu}{\lambda-\mu exp\left(-\left(\lambda-\mu\right)\left(T-t\right)\right)}\label{eq:NeeEtAlEq2}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $P\left(\lambda,\mu,t,T\right)$
\end_inset

 is the chance a lineage alive at time 
\begin_inset Formula $t$
\end_inset

 is still present at time 
\begin_inset Formula $T$
\end_inset

.
 The likelihood function, equation 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:NeeEtAlEq20"

\end_inset

, is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathcal{L}(\lambda,\mu,ts,T)=\left(N-1\right)!\left[\prod_{i=3}^{N}P(\lambda,\mu,t_{i},T)\right]\left(1-u(\lambda,\mu,x_{2})\right)^{2}\left[\prod_{i=3}^{N}\left(1-u(\lambda,\mu,x_{i})\right)\right]\label{eq:NeeEtAlEq20}
\end{equation}

\end_inset


\end_layout

